extern fn assert(value: bool);

# Sum 1 to 10
var sum = 0;
var i = 1;
loop {
    if i > 10 {
        break;
    }
    sum = sum + i;
    i = i + 1;
}
assert(sum == 55);

# Count with continue
var count = 0;
var j = 0;
loop {
    j = j + 1;
    if j > 10 {
        break;
    }
    if j %% 2 == 0 {
        continue;
    }
    count = count + 1;
}
assert(count == 5);  # odd numbers 1,3,5,7,9
