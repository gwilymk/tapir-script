extern fn assert(value: bool);

global counter = 0;
global multiplier = 10;

fn increment() {
    counter = counter + 1;
}

fn get_scaled() -> int {
    return counter * multiplier;
}

assert(counter == 0);
increment();
assert(counter == 1);
increment();
increment();
assert(counter == 3);
assert(get_scaled() == 30);
