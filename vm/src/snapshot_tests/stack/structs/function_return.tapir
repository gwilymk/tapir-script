struct Point { x: int, y: int }
property int_prop: int;

fn make_point(x: int, y: int) -> Point {
    return Point(x, y);
}

fn double_point(p: Point) -> Point {
    return Point(p.x * 2, p.y * 2);
}

var p = make_point(3, 4);
int_prop = p.x;
wait;
int_prop = p.y;
wait;
var q = double_point(p);
int_prop = q.x;
wait;
int_prop = q.y;
