struct Point { x: int, y: int }
property int_prop: int;

fn black_box(x: int) -> int {
    return x;
}

var p = Point(5, 10);
var q = p;
int_prop = black_box(q.x);
wait;
int_prop = black_box(q.y);
wait;
# Modify p, q should be unchanged (value semantics)
p.x = 999;
int_prop = black_box(q.x);
wait;
int_prop = black_box(p.x);
