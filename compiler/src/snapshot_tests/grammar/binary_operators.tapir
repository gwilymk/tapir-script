# Arithmetic operators
var a = 1 + 2 * 3;
var b = (1 + 2) * 3;
var c = 10 / 2 + 3;
var d = 10 // 3 %% 2;

# Comparison operators
var cmp1 = 1 < 2;
var cmp2 = 1 <= 2;
var cmp3 = 1 > 2;
var cmp4 = 1 >= 2;
var cmp5 = 1 == 2;
var cmp6 = 1 != 2;

# Logical operators - && binds tighter than ||
var logic1 = true && false;
var logic2 = true || false;
var logic3 = true || false && true;
var logic4 = (true || false) && true;

# Chained logical operators
var chain1 = true && true && true;
var chain2 = false || false || true;

# Mixed precedence: comparison < && < ||
var mixed1 = 1 < 2 && 3 < 4;
var mixed2 = 1 < 2 || 3 < 4;
var mixed3 = 1 < 2 && 3 < 4 || 5 < 6;

# Bitwise operators
var shl = 1 << 3;
var shr = 16 >> 2;
var band = 5 & 3;
var bor = 5 | 3;

# Bitwise precedence: add/sub < shift < bitand < bitor < comparison
# 1 + 2 << 3 should be (1 + 2) << 3 = 24 (shifts are looser than arithmetic)
var bit_prec1 = 1 + 2 << 3;
# 3 | 4 & 5 should be 3 | (4 & 5) = 3 | 4 = 7 (& binds tighter than |)
var bit_prec2 = 3 | 4 & 5;
# Chained shifts
var bit_chain = 1 << 2 << 1;
