# Field access on function return value

struct Point {
    x: int,
    y: int,
}

fn make_point() -> Point {
    return Point(10, 20);
}

fn test_field_on_call() {
    var x = make_point().x;
    var y = make_point().y;
}

fn test_field_on_call_in_expression() {
    var sum = make_point().x + make_point().y;
}
