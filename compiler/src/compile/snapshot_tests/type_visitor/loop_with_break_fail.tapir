fn foo() -> int {
    loop {
        if true {
            break;
        }

        return 4;
    }
}

fn nested() -> int {
    loop {
        if true {
            if true {
                break;
            }
        }

        return 1;
    }
}

fn multiple_paths() -> int {
    loop {
        if true {
            break;
        }

        if true {
            break;
        }

        return 1;
    }
}
